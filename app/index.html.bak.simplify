            <p>{{ item.notes }}</p>
            <p>{{ item.selected ? "---" : '' }}</p>
          </div>
        </md-list-item>
      </list->

      <item-renderer>
        <!--<md-list-item class="md-3-line" ng-repeat="item in items" ng-click="goTo(item)">-->
        <img ng-src="{{item.face}}?{{$index}}" class="md-avatar" alt="{{item.who}}" />
        <div class="md-list-item-text">
          <h3>{{ item.who }}</h3>
          <h4>{{ item.what }}</h4>
          <p>{{ item.notes }}</p>
          <p>{{ item.selected ? "---" : '' }}</p>
        </div>
        <!-- </md-list-item>-->
      </item-renderer>

    </quick-list>




    <quick-list ng-dp="headerMenuItems"
